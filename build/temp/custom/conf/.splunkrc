# Host at which Splunk is reachable (OPTIONAL)
host=localhost
# Port at which Splunk is reachable (OPTIONAL)
# Use the admin port, which is 8089 by default.
port=8089
# Splunk username
username=username
# Splunk password
password=password
# Access scheme (OPTIONAL)
scheme=https
# Namespace to use (OPTIONAL)
namespace=*:*
